{
	"schemaVersion": 1,
	"id": "crafty",
	"version": "${version}",
	"name": "${name}",
	"description": "${description}",
	"authors": [
		"Ryder Belserion"
	],
	"contact": {
		"homepage": "${website}",
		"sources": "${sources}",
		"issues": "${issues}"
	},
	"license": "MIT",
	"icon": "assets/crafty/icon.png",
	"environment": "*",
	"entrypoints": {
		"server": [
			"${group}.server.Crafty"
		]
	},
	"mixins": [
		{
			"config": "crafty.server.mixins.json",
			"environment": "server"
		}
	],
	"depends": {
		"fabricloader": ">=${fabricLoaderVersion}",
		"minecraft": "~${minecraftVersion}",
		"java": ">=17",
		"fabric-api": ">=${fabricApiVersion}"
	}
}